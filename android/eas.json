{
  "$schema": "https://eas.dev/eas.json/schema",
  "cli": {
    "version": ">= 16.1.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_ZEKE_BACKEND_URL": "https://zekeai.replit.app",
        "EXPO_PUBLIC_DOMAIN": "zekeai.replit.app:5000"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_ZEKE_BACKEND_URL": "https://zekeai.replit.app",
        "EXPO_PUBLIC_DOMAIN": "zekeai.replit.app:5000"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
