# ZEKE Docker deployment (.env for docker compose)
#
# Copy to .env (same folder as docker-compose.yml) and fill values.
# Never commit your real .env.

# --- Required secrets ---
OPENAI_API_KEY=
JWT_SECRET=
POSTGRES_PASSWORD=

# --- Optional but recommended ---
SESSION_SECRET=
APP_NAME=ZEKE
APP_ENV=production
LOG_LEVEL=info
PORT=5000

# --- Database naming (optional) ---
POSTGRES_DB=zeke
POSTGRES_USER=zeke
POSTGRES_PORT=5432

# --- Speech-to-text (optional) ---
DEEPGRAM_API_KEY=

# --- Omi (optional) ---
OMI_API_KEY=
OMI_DEV_API_KEY=
OMI_MCP_API_KEY=
OMI_COMMANDS_ENABLED=false

# --- Twilio (optional) ---
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_PHONE_NUMBER=
MASTER_ADMIN_PHONE=

# --- Internal/security (optional) ---
INTERNAL_BRIDGE_KEY=
EXPORT_SECRET_TOKEN=
OVERLAND_ACCESS_TOKEN=

# --- Briefings (optional) ---
MORNING_BRIEFING_ENABLED=false
MORNING_BRIEFING_PHONE=
