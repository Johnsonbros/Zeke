modules = ["nodejs-20", "python-3.11", "web", "bash", "python3", "postgresql-16"]
run = "npm run dev"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist"]

[nix]
channel = "stable-25_05"
packages = ["libxcrypt", "opensshWithKerberos", "sqlite"]

[deployment]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]
run = ["npm", "run", "start"]

[env]
PORT = "5000"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Trading Service"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[[workflows.workflow]]
name = "Trading Service"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python -m uvicorn zeke_trader.api:app --host 0.0.0.0 --port 8000"
waitForPort = 8000

[agent]
integrations = ["javascript_openai:1.0.0", "google-calendar:1.0.0", "twilio:1.0.0", "github:1.0.0", "javascript_object_storage:2.0.0", "javascript_database:1.0.0", "perplexity_v0:1.0.0"]
expertMode = true

[userenv]

[userenv.development]
INTERNAL_BRIDGE_KEY = "zeke-internal-bridge-2025"

[userenv.shared]
MASTER_ADMIN_PHONE = "6176868763"
ELEVENLABS_VOICE_ID = "7ZxKpU6EbWxrhZgWoRDv"
OPENAI_MODEL = "gpt-4o"
OPENAI_MINI_MODEL = "gpt-4o-mini"
APP_NAME = "zeke-family-assistant"
APP_ENV = "development"
LOG_LEVEL = "info"
MORNING_BRIEFING_ENABLED = "true"
MORNING_BRIEFING_PHONE = "+16176868763"
KG_ENABLED = "true"
VITE_KG_ENABLED = "true"
TRADING_MODE = "paper"
LIVE_TRADING_ENABLED = "false"
ALLOWED_SYMBOLS = "NVDA,SPY,META,GOOGL,AVGO,GOOG,AMZN"
MAX_DOLLARS_PER_TRADE = "25"
MAX_OPEN_POSITIONS = "3"
MAX_TRADES_PER_DAY = "5"
MAX_DAILY_LOSS = "25"
LOOP_SECONDS = "300"
LOG_DIR = "zeke_trader/logs"
AUTONOMY_TIER = "full_agentic"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 8000
externalPort = 8000
